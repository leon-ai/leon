    "login" : {
      "expressions": [
        "spotify login",
        "login to spotify"
      ]
    },
    "authorize" : {
      "expressions": [
        "http://localhost:1337/callback?code= "
      ],
      "entities": [
        {
          "type": "regex",
          "name": "auth_url",
          "regex": "^http://localhost:1337/callback?code="
        }
      ]
    }
